  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "clean": "rimraf node_modules dist",
    "re:build": "npm run clean && npm install && npm run build",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,html,css,scss,md}\"",
    "lint": "eslint --config ./eslint.config.js",
    "lint:fix": "eslint --fix --config ./eslint.config.js",
    "type-check": "tsc --noEmit",
    "validate": "npm run format && npm run lint && npm run type-check",
    "fix": "npm run format:fix && npm run lint:fix",
    "prepare": "husky"
  },
  "engines": {
    "node": "22.14.0"
  },
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "6.4.5",
