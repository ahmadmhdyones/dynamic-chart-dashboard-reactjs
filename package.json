    "react-router": "7.2.0",
    "recharts": "2.15.1",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@eslint/js": "9.21.0",
    "@tanstack/eslint-plugin-query": "5.66.1",
    "@types/lodash": "4.17.15",
